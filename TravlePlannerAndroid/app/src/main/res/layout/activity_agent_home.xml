<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ahmedmohamed.travleplanner.AgentHomeActivity"

    android:id="@+id/containerAgent"
    android:weightSum="10"
    android:orientation="vertical">

   
        <ImageView

            android:layout_marginBottom="20px"
            android:layout_marginRight="20px"
            android:elevation="5dp"
    android:src="@drawable/openmain"
    android:layout_width="100px"
    android:layout_height="100px"
    android:id="@+id/btn_open_agent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ouragentfragment">

    </FrameLayout>

    <com.hitomi.cmlibrary.CircleMenu

        android:id="@+id/circle_menu_agent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        />



</RelativeLayout>
